{
    "plugins": [ "joyent" ],
    "extends": [
        "eslint:recommended",
        "plugin:joyent/style",
        "plugin:joyent/lint"
    ],
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script",
        "ecmaFeatures": {
        }
    },
    "env": {
        "node": true
    },
    "rules": {
        "curly": ["error", "all"],
        "max-len": ["error", {
            "ignoreUrls": true //Not sure if this should be ignoreComments or ignoreUrls
        }]
    }
}
